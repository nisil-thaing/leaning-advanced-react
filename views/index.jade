doctype html(lang='en')
head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    link(rel='shortcut icon', href='favicon.ico?v=#{ staticVersion }')
    title #{ title }
    meta(name='theme-color', content='#000000')
    link(rel="stylesheet", href="main.bundle.css?v=#{ staticVersion }", attr)
  body
    #root.
      !{ initializeMarkup }
    script.
      window.__PRELOADED_STATE__ = !{ JSON.stringify(initialState).replace(/</g, '\\u003c') }
    script(type="application/javascript", src="main.bundle.js?v=#{ staticVersion }")
